{
  "name": "@foxpage/foxpage-cli",
  "version": "0.1.27",
  "description": "foxpage cli",
  "keywords": [
    "foxpage",
    "cli",
    "component",
    "server"
  ],
  "bin": {
    "foxpage": "bin/foxpage.js",
    "foxpage-server": "bin/foxpage-server.js",
    "foxpage-cmpt": "bin/foxpage-cmpt.js"
  },
  "author": {
    "name": "melody-yangjie",
    "email": "mryangjiekilitor@gmail.com",
    "url": "https://github.com/melodyJie"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/foxfamily/foxpage-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/foxfamily/foxpage-cli"
  },
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "tsc --skipLibCheck",
    "build:map": "tsc --skipLibCheck --sourceMap",
    "build:debug": "tsc --skipLibCheck --sourceMap --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --cache --cache-location=.cache/eslint --ext .ts,.tsx --report-unused-disable-directives ./src",
    "lint:fix": "yarn run lint --fix",
    "commit": "git-cz",
    "release": "standard-version --commitAll",
    "release:target": "yarn run release --release-as",
    "release:patch": "yarn run release:target patch",
    "release:minor": "yarn run release:target minor",
    "release:major": "yarn run release:target major",
    "release:alpha": "yarn run release --prerelease alpha",
    "release:beta": "yarn run release --prerelease beta",
    "prepublishOnly": "yarn build"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn build",
      "precommit": "git add ."
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ],
    "*.tsx": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@babel/polyfill": "^7.8.2",
    "@foxpage/foxpage-component-shared": "^0.1.12",
    "@foxpage/foxpage-component-webpack": "^0.1.24",
    "adm-zip": "^0.5.5",
    "autoprefixer": "^10.0.1",
    "chalk": "^4.1.1",
    "commander": "^6.0.0",
    "crypto-random-string": "^3.3.1",
    "execa": "^5.1.1",
    "filenamify": "^4.3.0",
    "folder-hash": "^4.0.1",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "got": "^11.8.2",
    "inquirer": "^8.1.0",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.9",
    "object-path": "^0.11.5",
    "open": "^7.0.2",
    "ora": "^5.4.1",
    "p-filter": "^2.1.0",
    "p-map": "^4.0.0",
    "postcss": "^8.1.2",
    "postcss-loader": "^4.0.4",
    "semver": "^7.1.3",
    "strong-log-transformer": "^2.1.0",
    "typescript": "^4.3.2",
    "typescript-json-schema": "^0.50.1",
    "uniqolor": "^1.0.1",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/config-lerna-scopes": "^8.1.0",
    "@types/adm-zip": "^0.4.34",
    "@types/folder-hash": "^4.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.2",
    "@types/less": "^3.0.3",
    "@types/lodash": "^4.14.177",
    "@types/mongodb": "^3.6.17",
    "@types/object-path": "^0.11.0",
    "@types/semver": "^7.1.3",
    "@types/strong-log-transformer": "^1.0.0",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "commitizen": "^3.1.0",
    "commitlint": "^8.1.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.2",
    "git-cz": "^4.7.6",
    "husky": "^4.3.8",
    "lint-staged": "^9.2.5",
    "package-json-type": "^1.0.3",
    "prettier": "^2.1.1",
    "standard-version": "^9.3.0"
  },
  "eslintIgnore": [
    "node_modules",
    "examples",
    "dist",
    "lib",
    "config",
    "babel.config.js",
    "jest.config.js",
    "commitlint.config.js",
    "*.js"
  ],
  "files": [
    "lib/**",
    "bin/**"
  ]
}
